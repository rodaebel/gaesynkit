[buildout]
develop = .
parts = python sphinx
versions = versions

[versions]
Jinja2 = 2.5.5
Pygments = 1.3.1
Sphinx = 1.0.5
WebOb = 1.0
WebTest = 1.2.3
coverage = 3.4
docutils = 0.7
setuptools = 0.6c9

[python]
recipe = zc.recipe.egg
eggs =
    WebTest
    coverage
    gaesynkit
extra-paths =
    @GAE_SDK@/
    @GAE_SDK@/lib/antlr3
    @GAE_SDK@/lib/django
    @GAE_SDK@/lib/fancy_urllib
    @GAE_SDK@/lib/webob
    @GAE_SDK@/lib/yaml/lib
interpreter = python

[sphinx]
recipe = zc.recipe.egg
eggs =
    gaesynkit
    sphinx
extra-paths =
    ${python:extra-paths}
